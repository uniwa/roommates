<xs:complexType name="student_roommate_type">
    <xs:complexContent>
        <xs:extension base="tns:student_type">
            <xs:sequence>
                <xs:element name="we_are" type="tns:emptyable_integer"/>
                <xs:element name="max_roommates" type="tns:emptyable_integer"/>
