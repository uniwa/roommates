<xs:simpleType name="afm_type">
    <xs:restriction base="xs:string">
        <xs:pattern value="\d{9}"/>
    </xs:restriction>
</xs:simpleType>

<!-- Postal Code -->
<xs:simpleType name="postal_code_type">
    <xs:restriction base="xs:string">
        <xs:pattern value="\d{5}|"/>
    </xs:restriction>
</xs:simpleType>

